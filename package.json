{
  "name": "strove",
  "private": true,
  "scripts": {
    "build": "npm run prisma migrate deploy && npm run prisma db seed && npm run build:css && remix build",
    "build:css": "tailwindcss -o ./app/styles.css",
    "clean": "git clean --force -dx --exclude .env.local --exclude .env.*.local --exclude node_modules/ --exclude .husky/",
    "dev": "concurrently \"npm run dev:css\" \"npm run dev:remix\"",
    "dev:css": "tailwindcss -o ./app/styles.css --watch",
    "dev:remix": "node -r dotenv/config node_modules/.bin/remix dev",
    "fix": "run-s fix:*",
    "fix:eslint": "npm run lint:eslint -- --fix",
    "fix:prettier": "npm run lint:prettier -- --write",
    "lint": "run-p lint:*",
    "lint:eslint": "eslint --ext .js,.ts,.tsx --ignore-path .gitignore .",
    "lint:prettier": "prettier --check --ignore-path .gitignore .",
    "lint:typescript": "tsc",
    "postinstall": "remix setup node",
    "prisma": "prisma",
    "prepare": "husky install",
    "test": "jest"
  },
  "lint-staged": {
    "*": "prettier --write --ignore-unknown",
    "*.{js,ts,tsx}": "eslint --fix"
  },
  "prisma": {
    "seed": "node --require esbuild-register prisma/seed.ts"
  },
  "dependencies": {
    "@headlessui/react": "^1.4.2",
    "@heroicons/react": "^1.0.5",
    "@prisma/client": "^3.7.0",
    "@react-aria/button": "^3.3.4",
    "@react-aria/ssr": "^3.1.0",
    "@react-aria/utils": "^3.11.0",
    "@remix-run/react": "^1.1.1",
    "@remix-run/serve": "^1.1.1",
    "@remix-run/vercel": "^1.1.1",
    "@splitbee/node": "^0.3.1",
    "@splitbee/web": "^0.3.0",
    "@tailwindcss/aspect-ratio": "^0.4.0",
    "@tailwindcss/forms": "^0.4.0",
    "@tailwindcss/line-clamp": "^0.3.0",
    "@tailwindcss/typography": "^0.5.0",
    "@tiptap/core": "^2.0.0-beta.160",
    "@tiptap/extension-document": "^2.0.0-beta.15",
    "@tiptap/extension-placeholder": "^2.0.0-beta.46",
    "@tiptap/react": "^2.0.0-beta.104",
    "@tiptap/starter-kit": "^2.0.0-beta.168",
    "bcryptjs": "^2.4.3",
    "chalk": "4",
    "envsafe": "^2.0.3",
    "inspx": "0.0.1-alpha.6",
    "match-sorter": "^6.3.1",
    "nanoid": "^3.1.30",
    "nprogress": "^0.2.0",
    "react": "^17.0.2",
    "react-beautiful-dnd": "^13.1.0",
    "react-dnd": "^14.0.5",
    "react-dnd-html5-backend": "^14.1.0",
    "react-dom": "^17.0.2",
    "react-polymorphic-types": "^2.0.0",
    "remix": "^1.1.1",
    "remix-auth": "^3.2.1",
    "remix-auth-form": "^1.1.1",
    "stripe": "^8.199.0",
    "tailwindcss": "^3.0.8",
    "tiny-invariant": "^1.2.0",
    "ts-toolbelt": "^9.6.0",
    "unstated-next": "^1.1.0",
    "web-vitals": "^2.1.4",
    "zod": "^3.11.6"
  },
  "devDependencies": {
    "@remix-run/dev": "^1.1.1",
    "@tsconfig/remix": "^1.0.0",
    "@types/bcryptjs": "^2.4.2",
    "@types/jest": "^27.4.0",
    "@types/nprogress": "^0.2.0",
    "@types/react": "^17.0.38",
    "@types/react-dom": "^17.0.11",
    "@types/tailwindcss": "^3.0.0",
    "@typescript-eslint/eslint-plugin": "^5.8.1",
    "@typescript-eslint/parser": "^5.8.1",
    "concurrently": "^6.5.1",
    "dotenv": "^10.0.0",
    "esbuild-register": "^3.3.1",
    "eslint": "^8.6.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^16.1.0",
    "eslint-config-next": "^12.0.7",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-etc": "^2.0.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "eslint-plugin-tailwindcss": "^3.1.2",
    "husky": "^7.0.4",
    "jest": "^27.4.5",
    "lint-staged": "^12.1.5",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.5.1",
    "prisma": "^3.7.0",
    "typescript": "^4.5.4"
  }
}
